@model IEnumerable<WebApplication1.Models.batch>

@{
    ViewBag.Title = "Index";
}


<hr />
<p>
    @Html.ActionLink("Add New Batch", "Create")
</p>
<table class="table" style=" border: 1px solid #ddd; padding: 8px; border-collapse: collapse; background-color: #96dff7">
    <tr style="background-color: #61bdf9">
        <th>
            @Html.DisplayNameFor(model => model.batch1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.year)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.batch1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.year)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
